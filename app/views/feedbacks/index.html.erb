<%= render "layouts/view_header", resource: @feedbacks.first %>

<table class="table table-hover table-curved table-condensed">
  <thead>
    <tr>
      <th>Match</th>
      <th>Is from receiver</th>
      <th>Match reported as Complete?</th>
      <th>Quality (1-5)</th>
      <th>Description</th>
      <th>Edit</th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.match&.name %></td>
        <td><%= yes_no(feedback.is_from_receiver) %></td>
        <td><%= yes_no(feedback.completed) %></td>
        <td><%= feedback.quality %></td>
        <td><%= feedback.description.truncate(30) %></td>
        <td><%= edit_button(feedback) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
