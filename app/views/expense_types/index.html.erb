<%= render "layouts/view_header", resource: @expense_types.first %>

<table class="table table-hover table-curved table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Edit</th>
    </tr>
  </thead>

  <tbody>
    <% @expense_types.each do |expense_type| %>
      <tr>
        <td><%= expense_type.name %></td>
        <td><%= expense_type.description %></td>
        <td><%= edit_button(expense_type) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
